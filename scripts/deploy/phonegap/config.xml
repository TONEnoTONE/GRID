<?xml version="1.0" encoding="UTF-8"?>	<widget xmlns = "http://www.w3.org/ns/widgets"		xmlns:gap = "http://phonegap.com/ns/1.0"		id        = "com.tonenotone.echoV1Staging"		versionCode=""		version   = "1.0.0">		<name>EchoStaging</name>		<description>Music Based Puzzler from TONEnoTONE</description>		<author href="http://tonenotone.com" email="tonenotone@gmail.com">TONEnoTONE</author>		<preference name="phonegap-version" value="3.3.0" />		<preference name="orientation" value="portrait" />		<preference name="fullscreen" value="true" />		<preference name="webviewbounce" value="true" />		<preference name="prerendered-icon" value="false" />		<preference name="stay-in-webview" value="false" />		<preference name="ios-statusbarstyle" value="default" />		<preference name="detect-data-types" value="true" />		<preference name="exit-on-suspend" value="false" />		<preference name="show-splash-screen-spinner" value="true" />		<preference name="auto-hide-splash-screen" value="true" />		<preference name="disable-cursor" value="false" />		<preference name="android-minSdkVersion" value="7" />		<preference name="android-installLocation" value="internalOnly" />		<preference name="splash-screen-duration" value="5000" />		<preference name="load-url-timeout" value="20000" />		<preference name="KeepRunning" value="true"/>		<preference name="DisallowOverscroll" value="false"/>				<gap:platform name="ios" />		<gap:plugin name="de.appplant.cordova.plugin.hidden-statusbar-overlay" version="1.0.0" />		<icon src="assets/images/gameIcon/EchoIcon57_nowords.png" gap:platform="ios" width="57" height="57" />		<icon src="assets/images/gameIcon/EchoIcon72_nowords.png" gap:platform="ios" width="72" height="72" />		<icon src="icon.png" />				<gap:splash src="assets/images/splash/ios/splash4_gradient.png" gap:platform="ios" width="320" height="480" />		<gap:splash src="assets/images/splash/splash4_gradient_tonenotone.png" gap:platform="ios" width="640" height="960" />		<gap:splash src="assets/images/splash/splash5_gradient_tonenotone.png" gap:platform="ios" width="640" height="1136" />		<gap:splash src="splash.png" />				<access origin="*" />		<feature name="Device">    	    <param name="ios-package" value="CDVDevice" />    	</feature>	</widget>